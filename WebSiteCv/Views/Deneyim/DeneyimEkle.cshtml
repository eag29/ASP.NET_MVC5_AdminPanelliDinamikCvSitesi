@using MvcCV.Models.Entity
@model List<TblDeneyimlerim>

@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />

<h4> Yeni Proje Ekleme Sayfası</h4>

<br />

<form method="post">

    @Html.Label(" Açıklama")
    @Html.TextBoxFor(x => x., new { @class = "form-control" })
    <br />

    @Html.Label("Başlık")
    @Html.TextBoxFor(x => x.Oran, new { @class = "form-control" })
    <br />

    <button class="btn btn-info">Kaydet</button>
</form>

